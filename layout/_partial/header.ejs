<div class="row">
  <div class="col-md-12">
    <header id="header">
      <a href="<%- url_for("/") %>" class="header-title">
        <img src="<%- url_for('images/logo.png') %>">
        <div id="title">
          <%= config.title %>
        </div>
      </a>
      <div id="nav-top">
      <ul>
        <% for (var i in theme.nav) { %> 
          <li><a href="<%- url_for(theme.nav[i]) %>"><%= i %></a></li>
        <% } %>
      </ul>
      </div>
    </header>
  </div>
</div>

